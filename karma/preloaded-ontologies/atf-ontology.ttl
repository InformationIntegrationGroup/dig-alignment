@prefix memex: <http://memexproxy.com/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix schema: <http://schema.org/> .
@prefix prov: <http://www.w3.org/ns/prov#> .

<http://memexproxy.com/ontology/> rdf:type owl:Ontology .


memex:hasReplyPart rdf:type owl:ObjectProperty ; 
	rdfs:label "hasReplyPart"@en ;
	rdfs:subPropertyOf schema:hasPart ;
	rdfs:comment "Identifies the reply part of a schema:CreativeWork."
	.

memex:isReplyPartOf rdf:type owl:ObjectProperty ; 
	rdfs:label "isReplyPartOf"@en ;
	owl:inverseOf memex:hasReplyPart ;
	rdfs:comment "Identifies the reply part of a schema:CreativeWork." 
	.

