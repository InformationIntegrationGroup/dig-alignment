@prefix dig: <http://dig.isi.edu/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2008/05/skos#> .
@prefix schema: <http://schema.org/> .

<http://dig.isi.edu/ontology/> rdf:type owl:Ontology .


#########################################################
#
# Classes
#
#########################################################


dig:URLEntity rdf:type owl:Class ; 
	rdfs:label "URLEntity"@en ;
	rdfs:comment """Represents an address on the Web from where we can retrieve a resource.
	Each time we dereference a URL we get a schema:CreativeWork, which we call a dig:snapshot"""
	.

dig:Identifier rdf:type owl:Class ; 
	rdfs:label "Identifier"@en ;
	rdfs:comment """An identifier of something."""
	.

dig:PhoneNumber rdf:type owl:Class ; 
	rdfs:subClassOf dig:Identifier ;
	rdfs:label "PhoneNumber"@en ;
	skos:note """Still wavering whether a phone number is a subclass of Identifier.""" ;
	rdfs:comment """A phone number."""
	.

dig:PersonName rdf:type owl:Class ; 
	rdfs:label "PersonName"@en ;
	skos:note """Defining as a class so that we can attach provenance to the object""" ;
	rdfs:comment """A person name."""
	.

dig:Ethnicity rdf:type owl:Class ; 
	rdfs:label "Ethnicity"@en ;
	rdfs:comment """Represents Identity with or membership in a particular racial, national,
	 or cultural group and observance of that group's customs, beliefs, and language."""
	.

dig:AgeGroup rdf:type owl:Class ; 
	rdfs:label "AgeGroup"@en ;
	rdfs:comment """Represents an age group, such as young, old, child."""
	.

dig:Preference rdf:type owl:Class ; 
	rdfs:label "Preference"@en ;
	rdfs:comment """Represents a preference that a Person or Organization may have
	about a certain subject."""
	.

dig:Boolean rdf:type owl:Class ; 
	rdfs:label "Boolean"@en ;
	rdfs:comment """Represents a Boolean value.
	The admissible instances are http://dig.isi.edu/data/boolean/true and falase."""
	.

#########################################################
#
# Object Properties
#
#########################################################

dig:snapshot rdf:type owl:ObjectProperty ; 
	rdfs:label "snapshot"@en ;
	rdfs:domain dig:URLEntity ; 
	rdfs:range schema:CreativeWork ;
	owl:inverseOf dig:isSnapshotOf ;
	rdfs:comment "Identifies a resource download from a url." 
	.

dig:isSnapshotOf rdf:type owl:ObjectProperty ; 
	rdfs:label "isSnapshotOf"@en ;
	rdfs:domain schema:CreativeWork ;
	rdfs:range dig:URLEntity ; 
	owl:inverseOf dig:snapshot ;
	rdfs:comment "Inverse of dig:snapshot." 
	.

dig:identifier rdf:type owl:ObjectProperty ; 
	rdfs:label "identifier"@en ;
	rdfs:range dig:Identifier ;
	rdfs:comment "An identifier of an entity." 
	.

dig:preferredIdentifier rdf:type owl:ObjectProperty ; 
	rdfs:subPropertyOf dig:identifier ;
	rdfs:label "preferredIdentifier"@en ;
	rdfs:range dig:Identifier ;
	rdfs:comment "The preferred identifier of an entity." 
	.

dig:hasTitlePart rdf:type owl:ObjectProperty ; 
	rdfs:label "hasTitlePart"@en ;
	rdfs:subPropertyOf schema:hasPart ;
	rdfs:comment "Identifies the title part of a schema:CreativeWork." 
	.

dig:isTitlePartOf rdf:type owl:ObjectProperty ; 
	rdfs:label "isTitlePartOf"@en ;
	owl:inverseOf  dig:hasTitlePart ;
	rdfs:comment "Identifies the title part of a schema:CreativeWork." 
	.

dig:hasBodyPart rdf:type owl:ObjectProperty ; 
	rdfs:label "hasBodyPart"@en ;
	rdfs:subPropertyOf schema:hasPart ;
	rdfs:comment "Identifies the body part of a schema:CreativeWork."
	.

dig:isBodyPartOf rdf:type owl:ObjectProperty ; 
	rdfs:label "isBodyPartOf"@en ;
	owl:inverseOf dig:hasBodyPart ;
	rdfs:comment "Identifies the body part of a schema:CreativeWork." 
	.

dig:hasImagePart rdf:type owl:ObjectProperty ; 
	rdfs:label "hasImagePart"@en ;
	rdfs:subPropertyOf schema:hasPart ;
	rdfs:range schema:ImageObject ;
	rdfs:comment "Identifies an ImageObject that is part of a schema:CreativeWork." 
	.

dig:isImagePartOf rdf:type owl:ObjectProperty ; 
	rdfs:label "isImagePartOf"@en ;
	owl:inverseOf dig:hasImagePart ;
	rdfs:range schema:ImageObject ;
	rdfs:comment "Identifies an ImageObject that is part of a schema:CreativeWork." 
	.

dig:mentionsPhoneNumber rdf:type owl:ObjectProperty ; 
	rdfs:label "mentionsPhoneNumber"@en ;
	rdfs:subPropertyOf schema:mentions ;
	rdfs:domain schema:CreativeWork ;
	rdfs:range dig:PhoneNumber ;
	rdfs:comment "Identifies an phone number mentioned in a schema:CreativeWork." 
	.

dig:isMentionedPhoneNumberIn rdf:type owl:ObjectProperty ; 
	rdfs:label "isMentionedPhoneNumberIn"@en ;
	rdfs:subPropertyOf schema:mentions ;
	rdfs:range schema:CreativeWork ;
	rdfs:domain dig:PhoneNumber ;
	owl:inverseOf dig:mentionsPhoneNumber;
	rdfs:comment "Identifies an phone number mentioned in a schema:CreativeWork." 
	.

dig:mentionsPlace rdf:type owl:ObjectProperty ; 
	rdfs:label "mentionsPlace"@en ;
	rdfs:subPropertyOf schema:mentions ;
	rdfs:range schema:Place ;
	rdfs:comment "Identifies a schema:Place mentioned in a schema:CreativeWork." 
	.

dig:isMentionedPlaceIn rdf:type owl:ObjectProperty ; 
	rdfs:label "isMentionedPlaceIn"@en ;
	rdfs:domain schema:Place ;
	owl:inverseOf dig:mentionsPlace;
	rdfs:comment "Identifies a schema:Place mentioned in a schema:CreativeWork." 
	.

dig:mentionsPerson rdf:type owl:ObjectProperty ; 
	rdfs:label "mentionsPerson"@en ;
	rdfs:subPropertyOf schema:mentions ;
	rdfs:range schema:Person ;
	rdfs:comment "Identifies a schema:Person mentioned in a schema:CreativeWork." 
	.

dig:isMentionedPersonIn rdf:type owl:ObjectProperty ; 
	rdfs:label "isMentionedPersonIn"@en ;
	rdfs:domain schema:Person ;
	owl:inverseOf dig:mentionsPerson ;
	rdfs:comment "Identifies a schema:Person mentioned in a schema:CreativeWork." 
	.

dig:urlEntity rdf:type owl:ObjectProperty ; 
	rdfs:label "urlEntity"@en ;
	rdfs:domain schema:CreativeWork ; 
	rdfs:range dig:URLEntity ;
	skos:note """schema.org has a schema:url property, but it is a DatatypeProperty, 
	and urlEntity is an ObjectProperty. We define the domain as schema:CreativeWork as opposed
	to owl:Thing as defined in schema.org.""" ;
	rdfs:comment "The URL of a schema:CreativeWork." 
	.

dig:primaryLocation rdf:type owl:ObjectProperty ; 
	rdfs:label "postingLocation"@en ;
	rdfs:subPropertyOf schema:mentions ;
	rdfs:domain schema:CreativeWork ;
	rdfs:range schema:Place ;
	rdfs:comment "The primary location associated with a schema:CreativeWork." 
	.

dig:pseudonym rdf:type owl:ObjectProperty ; 
	rdfs:label "pseudonym"@en ;
	rdfs:domain schema:Person ;
	rdfs:range dig:PersonName ;
	rdfs:comment "The pseudonym of a person." 
	.

dig:ethnicity rdf:type owl:ObjectProperty ; 
	rdfs:label "ethnicity"@en ;
	rdfs:domain schema:Person ;
	rdfs:range dig:Ethnicity ;
	rdfs:comment "The ethnicity of a person." 
	.

dig:preferenceQualifier rdf:type owl:ObjectProperty ; 
	rdfs:label "preferenceQualifier"@en ;
	rdfs:domain dig:Preference ;
	rdfs:comment """The qualifier of a preference. 
	The values in the range are URIs that form a controlled vocabulary;
	they include values such as http://..../preferencequalifier/only"""
	.

dig:preferenceSubject rdf:type owl:ObjectProperty ; 
	rdfs:label "preferenceSubject"@en ;
	rdfs:domain dig:Preference ;
	rdfs:comment """The thing about which a preference is stated. 
	The values in the range are URIs that form a controlled vocabulary;
	they include values such as http://..../agegroup/young"""
	.

#########################################################
#
# Data Properties
#
#########################################################

schema:url rdf:type owl:DatatypeProperty ;
	rdfs:label "url"@en ;
	skos:note """The owl file for schema.org is missing the url property, which is in schema.org.""" ;
	rdfs:comment "The URL of a resource." 
	.

dig:cacheUrl rdf:type owl:DatatypeProperty ;
	rdfs:label "cacheUrl"@en ;
	rdfs:subPropertyOf schema:url ;
	rdfs:comment "The URL of a cached version of a resource." 
	.

dig:mentionsPersonAge rdf:type owl:DatatypeProperty ;
	rdfs:label "cacheUrl"@en ;
	rdfs:domain schema:CreativeWork ;
	rdfs:comment "A person age mentioned in a schema:CreativeWork." 
	.

dig:tenDigitPhoneNumber rdf:type owl:DatatypeProperty ;
	rdfs:label "tenDigitPhoneNumber"@en ;
	rdfs:domain dig:PhoneNumber ;
	rdfs:comment "The 10 digit number of a USA phone number." 
	.
